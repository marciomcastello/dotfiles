[push]
    default = simple
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
[alias]
    a = add
    b = branch
    ci = commit -m
    cia = commit -am
    cl = clone
    co = checkout
    cob = checkout -b
    d = diff
    f = fetch --all
    g = grep
    heroku = push heroku master
    l = log
    m = merge
    pl = pull
    pop = stash pop
    pu = push
    r = rebase
    s = status
    st = stash
